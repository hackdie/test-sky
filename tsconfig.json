{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "jsx": "react",
    "lib": ["es7", "dom"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": false,
    "types": ["jest"],
    "target": "esnext",
    "typeRoots": ["node_modules/@types"]
  }
}
